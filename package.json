{
  "name": "logan-ui-lib-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "pnpm -r --filter ./packages/* run build",
    "dev": "pnpm -r --filter ./packages/* run dev",
    "test": "pnpm -r --filter ./packages/* run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "changeset": "changeset",
    "release": "changeset version && pnpm -r build && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "eslint": "^9.0.0",
    "prettier": "^3.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.2.9",
    "typescript": "^5.6.3",
    "vite": "^5.4.21"
  },
  "pnpm": {
    "overrides": {
      "esbuild": "0.21.5"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/logtheman/norby_ui.git"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "accessible",
    "typescript"
  ],
  "author": "Logan Murdock <logtheman@gmail.com>"
}